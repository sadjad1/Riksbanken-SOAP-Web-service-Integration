<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <customBinding>
                <binding name="SweaWebServiceSoap12Binding">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpsTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="https://swea.riksbank.se:443/sweaWS/services/SweaWebServiceHttpSoap12Endpoint"
                binding="customBinding" bindingConfiguration="SweaWebServiceSoap12Binding"
                contract="RiksbankenService.SweaWebServicePortType" name="SweaWebServiceHttpSoap12Endpoint" />
        </client>
    </system.serviceModel>
</configuration>